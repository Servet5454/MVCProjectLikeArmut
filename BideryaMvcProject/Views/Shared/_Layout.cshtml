<!doctype html>
<html class="no-js" lang="tr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Biderya</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico in the root directory -->
    <link rel="shortcut icon" type="image/x-icon" href="~/img/logo_favicon.png" />
    <!-- CSS here -->

    <link href="~/css/preloader.css" rel="stylesheet" />

    <link href="~/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/css/slick.css" rel="stylesheet" />

    <link href="~/css/meanmenu.css" rel="stylesheet" />

    <link href="~/css/owl.carousel.min.css" rel="stylesheet" />

    <link href="~/css/animate.min.css" rel="stylesheet" />

    <link href="~/css/backtotop.css" rel="stylesheet" />


    <link href="~/css/jquery.fancybox.min.css" rel="stylesheet" />

    <link href="~/css/fontawesome5pro.css" rel="stylesheet" />

    <link href="~/css/elegantfont.css" rel="stylesheet" />

    <link href="~/css/default.css" rel="stylesheet" />

    <link href="~/css/style.css" rel="stylesheet" />


</head>
<body>

    <!-- Add your site or application content here -->
    <!-- pre loader area start -->
    <div id="loading">
        <div id="loading-center">
            <div id="loading-center-absolute">
                <div class="object" id="object_one"></div>
                <div class="object" id="object_two" style="left:20px;"></div>
                <div class="object" id="object_three" style="left:40px;"></div>
                <div class="object" id="object_four" style="left:60px;"></div>
                <div class="object" id="object_five" style="left:80px;"></div>
            </div>
        </div>
    </div>
    <!-- pre loader area end -->
    <!-- back to top start -->
    <div class="progress-wrap">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
        </svg>
    </div>
    <!-- back to top end -->
    <!-- header area start -->
    <header>
        <div class="header__area white-bg" id="header-sticky">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xxl-2 col-xl-2 col-lg-2 col-md-4 col-6">
                        <div class="logo">
                            <a asp-controller="Home" asp-action="Anasayfa">
                                <img src="~/img/logo/logo_normal.jpg" alt="logo">
                            </a>
                        </div>
                    </div>
                    <div class="col-xxl-7 col-xl-7 col-lg-7 d-none d-lg-block offset-1">
                        <div class="main-menu">
                            <nav id="mobile-menu">
                                <ul>
                                    <li class="has-dropdown active">
                                        <a>Temizlik</a>
                                        <ul class="submenu">
                                            <li><a asp-controller="Temizlik" asp-action="">Ev Temizliği</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="KoltukTemizlik">Koltuk Yıkama</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Apartman Temizliği</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Halı Yıkama</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Haşere İlaçlama</a></li>
                                        </ul>
                                    </li>
                                    <li class="has-dropdown active">
                                        <a>Tadilat</a>
                                        <ul class="submenu">
                                            <li><a asp-controller="Temizlik" asp-action="">Boya Badana</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Ev Tadilatı</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Cam Balkon</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Mantolama</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Laminant Döşemne</a></li>
                                        </ul>
                                    </li>
                                    <li class="has-dropdown active">
                                        <a>Nakliyat</a>
                                        <ul class="submenu">
                                            <li><a asp-controller="Temizlik" asp-action="">Evden Eve</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Uluslararası</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Şehirler Arası</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Asansörlü Nakliyat</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Yurtdışı Kargo</a></li>
                                        </ul>
                                    </li>
                                    <li class="has-dropdown active">
                                        <a>Montaj Tamir</a>
                                        <ul class="submenu">
                                            <li><a asp-controller="Temizlik" asp-action="">Kombi Tamiri</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Mobilya Montajı</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Klima Montajı</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Su Kaçağı Tespiti</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Asansör Bakımı</a></li>
                                        </ul>
                                    </li>
                                    <li class="has-dropdown active">
                                        <a asp-controller="Kullanici" asp-action="Ilanlarim">Organizasyon</a>
                                        <ul class="submenu">
                                            <li><a asp-controller="Temizlik" asp-action="">Düğün-Nişan</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Doğum Günü</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Evlilik Teklifi</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Yat Kiralama</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="">Catering</a></li>
                                        </ul>
                                    </li>

                                    <li><a asp-controller="Kullanici" asp-action="HizmetVerenHesapOlustur">İletişim</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>

                    <div class="col-xxl-2 col-xl-2 col-lg-2 col-md-8 col-6">
                        @if (User.Identity.IsAuthenticated)
                        {
                            <div class="header__action d-flex align-items-center justify-content-end">
                                <div class="header__login d-none d-sm-block">
                                    <a asp-controller="Kullanici" asp-action="Girisyap"><i class="far fa-unlock" ></i> Hesabım </a>

                                    

                                </div>

                                <div class="sidebar__menu d-lg-none">
                                    <div class="sidebar-toggle-btn" id="sidebar-toggle">
                                        <span class="line"></span>
                                        <span class="line"></span>
                                        <span class="line"></span>
                                    </div>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="header__action d-flex align-items-center justify-content-end">
                                <div class="header__login d-none d-sm-block">
                                    <a asp-controller="Kullanici" asp-action="Girisyap"><i class="far fa-unlock"></i> Giriş Yap </a>
                                   

                                </div>

                                <div class="sidebar__menu d-lg-none">
                                    <div class="sidebar-toggle-btn" id="sidebar-toggle">
                                        <span class="line"></span>
                                        <span class="line"></span>
                                        <span class="line"></span>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- header area end -->
    <!-- sidebar area start -->
    <div class="sidebar__area">
        <div class="sidebar__wrapper">
            <div class="sidebar__close">
                <button class="sidebar__close-btn" id="sidebar__close-btn">
                    <span><i class="fal fa-times"></i></span>
                    <span>Kapat</span>
                </button>
            </div>
            <div class="sidebar__content">
                <div class="logo mb-40">
                    <a href="index.html">
                        <img src="~/img/logo/logo-white.png" alt="logo">
                    </a>
                </div>
                <div class="mobile-menu"></div>
                <div class="sidebar__action mt-330">
                    <div class="sidebar__login mt-15">
                        <a asp-action="GirisYap" asp-controller="Kullanici"><i class="far fa-unlock"></i>Giriş Yap</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- sidebar area end -->
    <div class="body-overlay"></div>
    <!-- sidebar area end -->
    @RenderBody();

    <!-- footer area start -->
    <footer>
        <div class="footer__area footer-bg">
            <div class="footer__top pt-90 pb-50">
                <div class="container">
                    <div class="row">
                        <div class="col-xxl-3 col-xl-3 col-lg-3 col-md-3 col-sm-4">
                            <div class="footer__widget mb-40 wow fadeInUp" data-wow-delay=".3s">
                                <div class="footer__widget-head mb-35">
                                    <h4 class="footer__widget-title">Sosyal Medya Hesaplarımız</h4>
                                </div>
                                <div class="footer__widget-content">
                                    <div class="footer__social mb-30">
                                        <ul>
                                            <li><a href="#" class="fb"><i class="fab fa-facebook-f"></i></a></li>
                                            <li><a href="#" class="tw"><i class="fab fa-twitter"></i></a></li>
                                            <li><a href="#" class="pin"><i class="fab fa-pinterest-p"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xxl-2 col-xl-2 col-lg-2 col-md-2 col-sm-4">
                            <div class="footer__widget mb-40 wow fadeInUp" data-wow-delay=".5s">
                                <div class="footer__widget-head">
                                    <h4 class="footer__widget-title">Ürünler</h4>
                                </div>
                                <div class="footer__widget-content">
                                    <div class="footer__link">
                                        <ul>
                                            <li><a asp-controller="Temizlik" asp-action="KoltukTemizlik">Koltuk Temizleme</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="BoyaBadanaIsleri">Boya-Badana Hizmeti</a></li>
                                            <li><a asp-controller="Temizlik" asp-action="EvdenEveNakliyat">Evden Eve Nakliyat</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xxl-3 col-xl-3 col-lg-3 col-md-3 col-sm-4">
                            <div class="footer__widget mb-40 wow fadeInUp footer__pl" data-wow-delay=".7s">
                                <div class="footer__widget-head">
                                    <h4 class="footer__widget-title"> <a asp-controller="Bilgi" asp-action="Hakkimizda"> Hakkımızda </a></h4>
                                </div>
                                <div class="footer__widget-content">
                                    <div class="footer__link">
                                        <ul>
                                            <li><a asp-controller="Bilgi" asp-action="Hakkimizda">Biderya Nedir?</a></li>
                                            <li><a asp-controller="" asp-action="">İletişim</a></li>
                                            <li><a asp-controller="" asp-action="">Destek</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xxl-2 col-xl-2 col-lg-2 col-md-2 col-sm-4">
                            <div class="footer__widget mb-40 wow fadeInUp" data-wow-delay=".9s">
                                <div class="footer__widget-head">
                                    <h4 class="footer__widget-title">Bilgi</h4>
                                </div>
                                <div class="footer__widget-content">
                                    <div class="footer__link">
                                        <ul>
                                            <li><a asp-controller="" asp-action="">Blog</a></li>
                                            <li><a asp-controller="Kullanici" asp-action="HesapOlustur">Üye Ol</a></li>
                                            <li><a asp-controller="" asp-action="">Bilgi Al</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer__bottom">
                <div class="container">
                    <div class="footer__bottom-inner">
                        <div class="row">
                            <div class="col-xxl-6 col-xl-6 col-md-6">
                                <div class="footer__copyright wow fadeInUp" data-wow-delay=".5s">
                                    <p>Tüm Hakları Saklıdır © 2023</p>
                                </div>
                            </div>
                            <div class="col-xxl-6 col-xl-6 col-md-6">
                                <div class="footer__bottom-link wow fadeInUp text-md-end" data-wow-delay=".8s">
                                    <ul>
                                        <li><a asp-controller="" asp-action="">destek@biderya.com</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- footer area end -->
    <!-- JS here -->

    <script src="~/js/vendor/jquery-3.5.1.min.js"></script>

    <script src="~/js/vendor/waypoints.min.js"></script>

    <script src="~/js/bootstrap.bundle.min.js"></script>

    <script src="~/js/jquery.meanmenu.js"></script>

    <script src="~/js/slick.min.js"></script>

    <script src="~/js/jquery.fancybox.min.js"></script>

    <script src="~/js/isotope.pkgd.min.js"></script>

    <script src="~/js/parallax.min.js"></script>

    <script src="~/js/owl.carousel.min.js"></script>

    <script src="~/js/backtotop.js"></script>

    <script src="~/js/jquery.counterup.min.js"></script>

    <script src="~/js/ajax-form.js"></script>

    <script src="~/js/wow.min.js"></script>

    <script src="~/js/imagesloaded.pkgd.min.js"></script>

    <script src="~/js/main.js"></script>
</body>
</html>

